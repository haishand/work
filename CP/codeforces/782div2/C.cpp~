#include<bits/stdc++.h>
using namespace std;
using ll = long long;
#define ar array

const int maxn = 1e5 + 10;
const ll mod = 1e9+7;

#define fi first
#define se second

int main() {
	int n;
	cin >> n;

	vector<int> s(n+1);
	vector<int> sum(n+1, 0);
	for(int i=1; i<=n; i++) {
		cin >> s[i];
		sum[i] = sum[i-1]+s[i];
	}

	int q;
	cin >> q;
	for(int i=0; i<q; i++) {
		int l, r;
		cin >> l >> r;

		/*
		int step = 2;
		int candies = 0;
		while(step<=n) {
			for(int i=l; i+step-1<=r; i+=step) {
				if(sum[i+step-1]-sum[i-1]>=10*(step/2)) {
					++candies;
				}
			}
			step *= 2;
		}
		*/
		int candies = 0;
		candies = (sum[r]-sum[l-1])/10;
		cout << candies << endl;
	}
	return 0;
}
