#include<bits/stdc++.h>
using namespace std;

int main() {
    int N;
	cin >> N;
   	vector<int> cnt(26);
    for(int i=0; i<N; i++) {
		string s;
		cin >> s;
		int len = s.size();
        int ans = 0;
        for(int j=0; j<len; j++) {
			for(int k=0; k<s[j]-'a'; k++) {
	            ans += cnt[k];
			}
        }
		cout << ans << endl;
        for(int j=0; j<len; j++) {
            ++cnt[s[j]-'a'];
        }
    }
    return 0;
}
