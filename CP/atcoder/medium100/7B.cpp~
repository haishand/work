#include<bits/stdc++.h>
using namespace std;

using ll = long long;

int main() {
	int n;
	cin >> n;
	string s;
	cin >> s;
	int ans = 0;
	for(int i=1; i<n; i++) {
		vector<bool> flag(26);
		int cnt = 0;
		for(int j=0; j<i; j++) {
			flag[s[j]-'a'] = true;
		}
		for(int j=i; j<n; j++) {
			if(flag[s[j]-'a']) {
				++cnt;
			}
		}
		ans = max(ans, cnt);
	}
	cout << ans << endl;
	return 0;
}
