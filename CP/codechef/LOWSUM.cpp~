#include <bits/stdc++.h>
using namespace std;
using ll = long long;

// too large memory allocation, so runtime error (other)
vector<ll> a, b, c;
int main() {
	int t;
	cin >> t;
	while(t--) {
		int k, q;
		cin >> k >> q;
		for(int i=0; i<k; i++) {
			int ta;
			cin >> ta;
			a.push_back(ta);
		}
		for(int i=0; i<k; i++) {
			int tb;
			cin >> tb;
			b.push_back(tb);
		}
		
		sort(a.begin(), a.end());
		sort(b.begin(), b.end());

		for(int i=0; i<min(k, 10000); i++) {
			for(int j=0; j<min(k, 10000/(k+1)); j++) {
				c.push_back(a[i]+b[j]);
			}
		}

		sort(c.begin(), c.end());

		for(int i=0; i<q; i++) {
			int qr;
			cin >> qr;
			cout << c[qr-1] << endl;
		}
		
	}
	return 0;
}

